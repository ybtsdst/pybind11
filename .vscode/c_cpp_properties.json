{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_POSIX_C_SOURCE=200809L",
                "_GNU_SOURCE=1"
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}